<p id="notice"><%= notice %></p>

<h1>Listing Humidity Readings</h1>

<table>
  <thead>
    <tr>
      <th>Sesnor ID</th>
      <th>Relative Humidity</th>
      <th>Measured At</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @humidity_readings.each do |humidity_reading| %>
      <tr>
        <td><%= humidity_reading.sensorID %></td>
        <td><%= humidity_reading.value %></td>
        <td><%= humidity_reading.measuredAt %></td>
        <td><%= link_to 'Show', humidity_reading %></td>
        <td><%= link_to 'Edit', edit_humidity_reading_path(humidity_reading) %></td>
        <td><%= link_to 'Destroy', humidity_reading, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Humidity reading', new_humidity_reading_path %>
